# Copy this file to .env and fill in your values
# API endpoint (Docker Compose maps backend to 3002 on host)
VITE_API_URL=http://localhost:3002

# Google Maps Platform API key
# Required APIs: Maps JavaScript API, Places API, Geocoding API
# Restrict to HTTP referrers including:
#   http://localhost:5173/*
#   http://127.0.0.1:5173/*
#   http://0.0.0.0:5173/* (optional for some setups)
VITE_GOOGLE_MAPS_API_KEY=REPLACE_WITH_YOUR_KEY

# Set to true to force mock map (disables Google Maps load)
VITE_MOCK_MAPS=false

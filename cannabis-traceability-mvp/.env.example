### Example env for local development and notes for VPS
# Keycloak admin user (change for production)
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin

# OAuth2 client secret for oauth2-proxy (change before VPS deployment)
OAUTH2_PROXY_CLIENT_SECRET=changeme

# 32 byte base64 cookie secret (required). Generate one:
# python -c 'import os,base64;print(base64.b64encode(os.urandom(32)).decode())'
COOKIE_SECRET=changeme-cookie-secret-base64

# Google maps key used by frontends (if needed)
VITE_GOOGLE_MAPS_API_KEY=

# Copy to .env and fill with strong values before deploying
# Keycloak admin
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=replace-with-strong-password

# oauth2-proxy / OIDC
OAUTH2_PROXY_OIDC_ISSUER_URL=https://auth.renewedge-solutions.com/realms/your-realm
OAUTH2_PROXY_CLIENT_ID=mvp-client
OAUTH2_PROXY_CLIENT_SECRET=replace-with-client-secret
OAUTH2_PROXY_COOKIE_SECRET=replace-with-32-or-64-byte-base64-secret
OAUTH2_PROXY_REDIRECT_URL=https://s2s-mvp.renewedge-solutions.com/oauth2/callback

# Additional (optional)
# Ensure you do not commit real secrets to version control
